{
  "bindings": [
    {
        "authLevel": "function",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "route": "{*route}",
        "methods": [
          "get",
          "post",
          "options",
          "trace",
          "delete",
          "put",
          "connect",
          "head",
          "patch"
        ]
      },
      {
        "type": "http",
        "direction": "out",
        "name": "res"
      }
  ],
  "scriptFile": "./index.js",
  "entryPoint": "default"
}